{
  "name": "segmented-property",
  "version": "2.0.4",
  "description": "A utility for immutably setting and getting properties on JavaScript objects as slash-delimited paths.",
  "author": "Marius Lundg√•rd <studio@mariuslundgard.com>",
  "license": "MIT",
  "main": "./dist/commonjs/index.js",
  "module": "./dist/module/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:es": "tsc -p tsconfig.es.json",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .js,.ts",
    "postinstall": "yarn husky install",
    "postpublish": "pinst --enable",
    "prebuild": "yarn clean",
    "prepublishOnly": "pinst --disable && yarn build",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "pinst": "^2.1.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.3.9",
    "semantic-release-cli": "^5.4.3",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mariuslundgard/segmented-property.git"
  }
}
